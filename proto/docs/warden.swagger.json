{
  "swagger": "2.0",
  "info": {
    "title": "warden.proto",
    "version": "version not set"
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/trainer/{trainer.uid}/zone/{zone.opt_code}": {
      "get": {
        "summary": "Encounter might add a pokemon to the event",
        "description": "A pokemon will be added to the event and timestmap set if one is encountered",
        "operationId": "Enter",
        "responses": {
          "200": {
            "description": "Description",
            "schema": {
              "$ref": "#/definitions/wardenTicket"
            }
          }
        },
        "parameters": [
          {
            "name": "trainer.uid",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "string"
          },
          {
            "name": "zone.opt_code",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "Warden"
        ]
      }
    }
  },
  "definitions": {
    "PokemonCollection": {
      "type": "object",
      "properties": {
        "pokemon": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/pokedexPokemon"
          }
        }
      },
      "title": "Pokemon_Collection is self evident"
    },
    "TicketExpiry": {
      "type": "object",
      "properties": {
        "encounters": {
          "type": "integer",
          "format": "int32"
        },
        "time": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "TrainerGender": {
      "type": "string",
      "enum": [
        "BOY",
        "GIRL"
      ],
      "default": "BOY"
    },
    "ZoneCode": {
      "type": "string",
      "enum": [
        "KANTO",
        "JOHTO",
        "HOENN",
        "SINNOH",
        "UNOVA",
        "KALOS",
        "ALOLA"
      ],
      "default": "KANTO"
    },
    "msgTimestamp": {
      "type": "object",
      "properties": {
        "unix": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "pokedexPokemon": {
      "type": "object",
      "properties": {
        "catch_rate": {
          "type": "integer",
          "format": "int32",
          "title": "CatchRate is chance of catching on a scale of 1-255"
        },
        "name": {
          "type": "string",
          "format": "string",
          "title": "Name is the English name of the pokemon"
        },
        "nick_name": {
          "type": "string",
          "format": "string",
          "title": "NickName is the name this pokemon has been given by it's trainer"
        },
        "number": {
          "type": "integer",
          "format": "int32",
          "title": "Number is the canonical pokemon identifier"
        },
        "speed": {
          "type": "integer",
          "format": "int32",
          "title": "Speed is how fast the pokemon is on a scale 1-100"
        },
        "type": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/pokedexPokemonType"
          },
          "title": "Type is the type of this pokemon"
        },
        "uid": {
          "type": "string",
          "format": "string",
          "title": "Uid is the unique identifier for this individual pokemon"
        }
      },
      "title": "Pokemon message is info about a pokemon"
    },
    "pokedexPokemonType": {
      "type": "string",
      "enum": [
        "UNKNOWN",
        "NORMAL",
        "FIGHTING",
        "FLYING",
        "POISON",
        "GROUND",
        "ROCK",
        "BUG",
        "GHOST",
        "STEEL",
        "FIRE",
        "WATER",
        "GRASS",
        "ELECTRIC",
        "PSYCHIC",
        "ICE",
        "DRAGON",
        "DARK",
        "FAIRY"
      ],
      "default": "UNKNOWN",
      "title": "Pokemon_Type is the canonical type of a pokemon"
    },
    "registryTrainer": {
      "type": "object",
      "properties": {
        "age": {
          "type": "integer",
          "format": "int32"
        },
        "gender": {
          "$ref": "#/definitions/TrainerGender"
        },
        "name": {
          "type": "string",
          "format": "string"
        },
        "password": {
          "type": "string",
          "format": "string"
        },
        "pc": {
          "$ref": "#/definitions/PokemonCollection"
        },
        "scope": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "string"
          }
        },
        "start": {
          "$ref": "#/definitions/msgTimestamp"
        },
        "uid": {
          "type": "string",
          "format": "string"
        }
      }
    },
    "wardenTicket": {
      "type": "object",
      "properties": {
        "expires": {
          "$ref": "#/definitions/TicketExpiry"
        },
        "time": {
          "$ref": "#/definitions/msgTimestamp"
        },
        "trainer": {
          "$ref": "#/definitions/registryTrainer"
        },
        "uid": {
          "type": "string",
          "format": "string"
        },
        "zone": {
          "$ref": "#/definitions/wardenZone"
        }
      }
    },
    "wardenZone": {
      "type": "object",
      "properties": {
        "opt_code": {
          "type": "integer",
          "format": "int32"
        },
        "region": {
          "$ref": "#/definitions/ZoneCode"
        }
      }
    }
  }
}
